run = "cd bolt-timetracker && npm run dev"
entrypoint = "bolt-timetracker/src/main.tsx"

[env]
PATH = "/home/runner/$REPL_SLUG/.config/npm/node_global/bin:/home/runner/$REPL_SLUG/node_modules/.bin:$PATH"
npm_config_prefix = "/home/runner/$REPL_SLUG/.config/npm/node_global"

[nix]
channel = "stable-23_11"

[deployment]
run = ["sh", "-c", "cd bolt-timetracker && npm run preview"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 5000
externalPort = 80
